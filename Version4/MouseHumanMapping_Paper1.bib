	file = {Fulcher et al. - 2019 - Multimodal gradients across mouse cortex.pdf:/Users/work/Zotero/storage/PIEY4DDC/Fulcher et al. - 2019 - Multimodal gradients across mouse cortex.pdf:application/pdf},
}

