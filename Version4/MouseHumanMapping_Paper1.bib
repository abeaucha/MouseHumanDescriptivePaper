@article{balsters_primate_2020,
	title = {Primate homologs of mouse cortico-striatal circuits},
	volume = {9},
	issn = {2050-084X},
	url = {https://elifesciences.org/articles/53680},
	doi = {10.7554/eLife.53680},
	abstract = {With the increasing necessity of animal models in biomedical research, there is a vital need to harmonise findings across species by establishing similarities and differences in rodent and primate neuroanatomy. Using connectivity fingerprint matching, we compared cortico-striatal circuits across humans, non-human primates, and mice using resting-state fMRI data in all species. Our results suggest that the connectivity patterns for the nucleus accumbens and cortico-striatal motor circuits (posterior/lateral putamen) were conserved across species, making them reliable targets for cross-species comparisons. However, a large number of human and macaque striatal voxels were not matched to any mouse cortico-striatal circuit (mouse-{\textgreater}human: 85\% unassigned; mouse-{\textgreater}macaque 69\% unassigned; macaque-{\textgreater}human; 31\% unassigned). These unassigned voxels were localised to the caudate nucleus and anterior putamen, overlapping with executive function and social/language regions of the striatum and connected to prefrontal-projecting cerebellar lobules and anterior prefrontal cortex, forming circuits that seem to be unique for non-human primates and humans.},
	language = {en},
	number = {e53680},
	urldate = {2021-11-22},
	journal = {eLife},
	author = {Balsters, Joshua Henk and Zerbi, Valerio and Sallet, Jerome and Wenderoth, Nicole and Mars, Rogier B},
	month = apr,
	year = {2020},
	pages = {24},
	file = {Balsters et al. - 2020 - Primate homologs of mouse cortico-striatal circuit.pdf:/Users/work/Zotero/storage/ULX99RSM/Balsters et al. - 2020 - Primate homologs of mouse cortico-striatal circuit.pdf:application/pdf},
}

