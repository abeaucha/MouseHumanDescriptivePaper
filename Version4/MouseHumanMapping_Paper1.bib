	file = {Ji et al. - 2014 - Integrative analysis of the connectivity and gene .pdf:/Users/work/Zotero/storage/AXWSSVCV/Ji et al. - 2014 - Integrative analysis of the connectivity and gene .pdf:application/pdf},
}

@article{fulcher_transcriptional_2016,
	title = {A transcriptional signature of hub connectivity in the mouse connectome},
